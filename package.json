    "@prisma/client": "^5.7.1",
    "axios": "^1.3.4",
    "axios-retry": "^3.4.0",
    "pretty-bytes": "^6.1.1",
    "svelte-icons": "^2.1.0",
    "svelte-markdown": "^0.2.3",
    "svelte-typewriter": "^3.1.6"
  }
