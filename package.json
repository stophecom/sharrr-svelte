    "@aws-sdk/s3-presigned-post": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.678.0",
    "@prisma/client": "^6.3.0",
    "axios": "^1.7.8",
    "axios-retry": "^4.5.0",
    "pretty-bytes": "^6.1.1",
    "svelte-icons": "^2.1.0",
    "svelte-markdown": "^0.4.1",
    "svelte-typewriter": "^3.1.6"
  }
